{
  "name": "@shadowmesh/sdk",
  "version": "0.1.0",
  "description": "SDK for deploying censorship-resistant applications to ShadowMesh",
  "type": "module",
  "bin": {
    "shadowmesh": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "cli": "node dist/cli.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@storacha/client": "^1.0.0",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "blake3-wasm": "^2.1.5"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "shadowmesh",
    "decentralized",
    "cdn",
    "ipfs",
    "web3",
    "censorship-resistant"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Antismart/shadowmesh"
  }
}
