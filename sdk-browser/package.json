{
  "name": "@shadowmesh/browser",
  "version": "0.1.0",
  "description": "ShadowMesh browser SDK with WebRTC support",
  "type": "module",
  "main": "./pkg/shadowmesh_browser.js",
  "types": "./pkg/shadowmesh_browser.d.ts",
  "files": [
    "pkg/*"
  ],
  "scripts": {
    "build": "wasm-pack build --target web --out-dir pkg",
    "build:nodejs": "wasm-pack build --target nodejs --out-dir pkg-node",
    "build:bundler": "wasm-pack build --target bundler --out-dir pkg-bundler",
    "test": "wasm-pack test --headless --chrome",
    "test:firefox": "wasm-pack test --headless --firefox",
    "clean": "rm -rf pkg pkg-node pkg-bundler target"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Antismart/shadowmesh"
  },
  "keywords": [
    "shadowmesh",
    "webrtc",
    "p2p",
    "decentralized",
    "cdn",
    "wasm",
    "browser"
  ],
  "author": "ShadowMesh Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Antismart/shadowmesh/issues"
  },
  "homepage": "https://github.com/Antismart/shadowmesh#readme"
}
